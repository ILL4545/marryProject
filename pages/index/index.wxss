page{
  background: #f0f0f0;
}
.search{
   background: #fb7299; 
  padding:0 2% 2% 2%;
  display: flex;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  width: 96%;

}
.search view{
  background: #fff;
  border-radius: 20px;
  display: flex;
  padding: 0 2%;
  width: 96%;
}
.search .icon{
  font-size: 14px;
  display: inline-block;
  height: 35px;
  line-height: 35px;
  color: #9b9b9b;
  margin: 0 5px 0 0;
}
.search input{
  font-size: 14px;
  height: 35px;
  line-height: 35px;
  width: 100%;
}
.search button{
  font-size: 14px;
  background: none;
  height: 35px;
  line-height: 35px;
  color: #fff;
  width: 60px;
  padding: 0;
}

.navbar {
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  border-bottom: 1px solid #f1f1f1;
  position: fixed;
  top: 42px;
  left: 0;
  z-index: 999;
  width: 100%;
}

.weui-navbar__item {
  position: relative;
  display: block;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  padding: 10px 0;
  text-align: center;
  font-size: 15px;

}
.navbar .active {
  /* color: #ec703a; */

}
.navbar__slider {
  position: absolute;
  content: " ";
  left: 0;
  bottom: 0;
  width:30px;
  height: 3px;
  border-radius: 50px;
  background-color:  #e4463b;
  -webkit-transition: -webkit-transform .3s;
  transition: -webkit-transform .3s;
  transition: transform .3s;
  transition: transform .3s, -webkit-transform .3s;

}

.navbar .weui-navbar__title {
  display: inline-block;
  font-size: 15px;
  max-width: 8em;
  width: auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal;
}

swiper{
  width: 94%;
  border-radius: 5px;
  overflow: hidden;
  margin:95px 3% 3% 3%;
  height: 200px;
}
swiper-item{
  border-radius: 5px;
}
swiper image{
   border-radius: 5px;
   width: 100%;
   height: 100%;
}

.img_text{
	background: #fff;
	/* margin-bottom: 10px; */
  border-bottom: 1px solid #f1f1f1;
}
.img_text .itlist{
	font-size: 16px;
	/* margin: 5px 0; */
	padding: 10px 0;
	border-bottom: 1px solid #f1f1f1;
}
.img_text .itlist:last-of-type{
	border-bottom: none;
}
.img_text .itlist text{
	padding: 0 3% 5px 3%;
	display: inline-block;
}
.img_text .itlist .piclist {
	display: flex;
	padding: 0 2%;
}
.img_text .itlist .piclist view{
	margin: 0 0.5%;
	width: 50%;
}
.img_text .itlist .piclist view image{
	width: 100%;
  border-radius: 2px;
}

.headview{
	background: #fff;
	padding: 15px 10px 10px 10px;
}
.headview view{
	display: inline-block;
	font-weight: bold;
	font-size: 18px;
}
.headview view:nth-of-type(2){
	float: right;
	font-weight: normal;
	font-size: 14px;
	color: #999;
}
.headview view:nth-of-type(2) text{
	font-size: 12px;
}
.xianshi{
	background: #fff;
	padding: 0 10px 10px 10px;
	overflow: hidden;
	overflow-x: scroll;
	white-space: nowrap;
    margin-bottom: 10px;
}
.xianshi view{
	display: inline-block;
	width: 33%;
	margin-right: 3%;
	font-size: 13px;
}
.xianshi view text{
	color: #d33c3e;
	font-weight: bold;
}
.xianshi view image{
	width: 100%;
	display: block;
	border-radius: 5px;
	box-shadow: 0 1px 10px gainsboro;
}

.gifts{
	/*background: #fff;*/
	padding: 15px 0;
}
.giftlist{
  background: #fff;
  box-shadow: 0px 1px 5px #d8d8d8;
  padding: 10px;
  border-radius: 5px;
  margin-bottom: 10px;
  margin: 0 2% 4% 2%;
  position: relative;
}
.giftlist .shopinfo{
	padding:0 0 5px 0;
}
.giftlist .shopinfo view:nth-of-type(2){
	float: right;
	font-size: 14px;
	color: #777;
  display: inline-block;
}
.giftlist .shopinfo view:nth-of-type(1){
	display: inline-block;
	font-size: 14px;
	color: #777;
}
.giftlist .shopinfo view:nth-of-type(1) text{
	margin-right: 5px;
	color: #444;
	font-size: 16px !important;
}
.giftlist .img image{
   width: 100%; 
    height:195px;
}
.giftlist .titlename{
  line-height: 25px;
  font-size: 13px;
  /* display: initial; */
  color: #777
}
.giftlist .price{
  /* margin-bottom:10px; */
  
}
.giftlist .price view:nth-of-type(1) {
  font-weight: bold;
  font-size: 18px;
  margin-right: 5px;
  display: inline-block;
}
.giftlist .price view:nth-of-type(1) text{
  color: #e4463b;
}
.giftlist .price view:nth-of-type(2){
  color: #777;
  font-size: 16px;
  text-decoration:line-through;
  display: inline-block;
}

.marryPrice{
  display: initial;
  padding-left: 222rpx;
}

.giftlist .goumai{
  background: #e64949;
  color: #fff;
  display: inline-block;
  font-size: 14px;
  width: 85px;
  border: none;
  height: 40px;
  line-height: 40px;
  position: absolute;
  right: 2%;
  bottom: 10px;
}


